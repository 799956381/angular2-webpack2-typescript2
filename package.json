{
  "name": "angular2-webpack",
  "version": "0.0.5",
  "description": "Angular2, Webpack2, Material Design, Typescript2, Docker, Sass, Codelyzer",
  "license": "MIT",
  "main": "",
  "engines": {
    "node": ">= 4.4.7",
    "npm": ">= 3"
  },
  "scripts": {
    "initialize": "npm run clean && docker-compose build",
    "clean": "rimraf dist/frontend",
    "tslint": "tslint ./src/frontend/app/**/*.ts",
    "serve": "webpack-dev-server --config webpack/webpack.dev.js --inline --progress --profile --watch --content-base src/frontend --host=0.0.0.0",
    "build": "rimraf dist/frontend && webpack --config webpack/webpack.prod.js  --progress --profile  --bail",
    "serve-dist": "http-server dist/frontend --cors -p 8080 ",
    "docker:install": "docker-compose run frontend npm install",
    "docker:serve-dist": "docker-compose run --service-ports frontend npm run serve-dist",
    "docker:build": "docker-compose run frontend npm run build",
    "docker:serve": "docker-compose run --service-ports frontend npm run serve",
    "docker:tslint": "docker-compose run --service-ports frontend npm run tslint",
    "docker:bash": "docker-compose run --service-ports frontend bash"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "^0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@types/core-js": "^0.9.28",
    "@types/hammerjs": "^2.0.28",
    "@types/node": "^4.0.29",
    "@types/source-map": "^0.1.26",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.29",
    "awesome-typescript-loader": "^2.0.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "codelyzer": "0.0.28",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.3",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
