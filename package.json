    "hammerjs": "2.0.8",
    "html-webpack-plugin": "2.16.0",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "ignore-loader": "^0.1.1",
    "image-webpack-loader": "^1.8.0",
    "imports-loader": "^0.6.5",
    "json-loader": "0.5.4",
    "less": "2.5.3",
